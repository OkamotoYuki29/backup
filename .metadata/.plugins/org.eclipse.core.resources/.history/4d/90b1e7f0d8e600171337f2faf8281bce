package free;

import java.util.ArrayList;

public class Main{
	public static void main(String[] args){
		//配列
		String[] s;
		s = new String[2];
		s[0] = "abc";
		System.out.println(s[0]);
		//ArrayList
		ArrayList<String> array;
		array = new ArrayList<String>(2);
		array.add("akira");
		System.out.println(array.get(0));
	}
}