package Free;

import java.io.InputStream;
import java.net.URL;

public class Main{
	public static void main(String[] args)throws Exception{
		URL url  new URL("https://www.yahoo.co.jp/");
		InputStream is = url.openStream();
		int i = is.read();
		while(i != -1){
			char c = (char) i;
			System.out.println(c);
			i = is.read();
		}
	}
}